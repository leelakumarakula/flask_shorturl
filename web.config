<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="PythonHandler"
           path="*"
           verb="*"
           modules="FastCgiModule"
           scriptProcessor="C:\Domains\vblue.link\flask_shorturl\venv\Scripts\python.exe|C:\Domains\vblue.link\flask_shorturl\venv\Lib\site-packages\wfastcgi.py"
           resourceType="Unspecified"
           requireAccess="Script" />
    </handlers>
  </system.webServer>

  <appSettings>
    <add key="PYTHONPATH" value="C:\Domains\vblue.link\flask_shorturl" />
    <add key="WSGI_HANDLER" value="wsgi.app" />
    <!-- <add key="WSGI_LOG" value="C:\Domains\vblue.link\flask_shorturl\wfastcgi.log" /> -->
  </appSettings>
</configuration>
