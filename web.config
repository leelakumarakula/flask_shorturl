<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="vbluelinktest" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\Domains\vblueapi\flask_shorturl\venv\Scripts\python.exe|C:\Domains\vblueapi\flask_shorturl\venv\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" />
      <add name="PythonHandler" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\Domains\vblue.link\flask_shorturl\venv\Scripts\python.exe|C:\Domains\vblue.link\flask_shorturl\venv\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" requireAccess="Script" />
    </handlers>
        <rewrite>
            <rules>
                <rule name="redirect to www" enabled="true" stopProcessing="true">
                    <match url="(.*)" />
                    <conditions>
                        <add input="{HTTP_HOST}" pattern="^vblue\.link$" ignoreCase="false" />
                        <add input="{CACHE_URL}" pattern="^(.+)://" ignoreCase="false" />
                    </conditions>
                    <action type="Redirect" url="{C:1}://www.vblue.link/{R:1}" />
                </rule>
            </rules>
        </rewrite>
  </system.webServer>

  <appSettings>
    <add key="PYTHONPATH" value="C:\Domains\vblue.link\flask_shorturl" />
    <add key="WSGI_HANDLER" value="wsgi.app" />
    <!-- <add key="WSGI_LOG" value="C:\Domains\vblue.link\flask_shorturl\wfastcgi.log" /> -->
  </appSettings>
</configuration>
